{
  "name": "grapesjs-dev-helper",
  "version": "1.0.0",
  "description": "GrapesJS plugin for the development",
  "main": "src/index.js",
  "bin": "bin/grapesjs-dev-helper",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artf/grapesjs-dev-helper.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "dev",
    "helper"
  ],
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "yargs": "^14.0.0"
  }
}
